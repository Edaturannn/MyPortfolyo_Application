@model ToDoList
@{
    ViewData["Title"] = "CreateToDoList";
    Layout = "~/Views/Shared/_Admins.cshtml";
}

<h1 style="text-align:center;">New Add</h1>
<form method="post">
    <label>Title</label>
    <input type="text" asp-for="Title" class="form-control" autocomplete="off" />
    <br />

    <label>ImageUrl</label>
    <input type="text" asp-for="ImageUrl" class="form-control" autocomplete="off" />
    <br />

    <label>Text</label>
    <textarea type="text" asp-for="Text" class="form-control" autocomplete="off" rows="5"></textarea>
    <br />
    <button class="btn btn-info">Create</button>
</form>

