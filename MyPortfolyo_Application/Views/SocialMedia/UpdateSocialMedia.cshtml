@model SocialMedia
@{
    ViewData["Title"] = "UpdateSocialMedia";
    Layout = "~/Views/Shared/_Admins.cshtml";
}

<h1 style="text-align:center;">Update</h1>
<form method="post">
    <input type="hidden" asp-for="SocialMediaId"/>
    <label>Title</label>
    <input type="text" asp-for="Title" class="form-control" autocomplete="off" />
    <br />

    <label>Url</label>
    <input type="text" asp-for="Url" class="form-control" autocomplete="off" />
    <br />

    <label>Icon</label>
    <input type="text" asp-for="Icon" class="form-control" autocomplete="off" />
    <br />
    <button class="btn btn-info">Update</button>
</form>

